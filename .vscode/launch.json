        {
            "type": "java",
            "name": "Launch GenerateAst",
            "request": "launch",
            "mainClass": "tool.GenerateAst",
            "projectName": "JLox",
            "args": "./src/main/java/JLox/"
        },
        {
            "type": "java",
            "name": "Launch Lox",
